{
  "name": "DataCapturing",
  "version": "0.0.0",
  "summary": "Framework used to continuously capture data from all available sensors on an iOS device and transmit it to a Cyface-API compatible server.",
  "description": "This framework can be included by your App if you are going to capture sensor data and transmit that data to a Cyface-API server for further analysis.",
  "homepage": "https://cyface.de",
  "license": {
    "type": "GPL",
    "file": "LICENSE"
  },
  "authors": "Cyface GmbH",
  "source": {
    "git": "https://github.com/cyface-de/ios-backend.git",
    "tag": "0.0.0"
  },
  "social_media_url": "https://twitter.com/CyfaceDE",
  "platforms": {
    "ios": "12.4"
  },
  "swift_versions": "5.3",
  "source_files": "DataCapturing/Source/**/*{.h,.m,.swift}",
  "resources": "DataCapturing/Source/**/*{.xcdatamodeld,.xcdatamodel,.xcmappingmodel}",
  "frameworks": [
    "CoreData",
    "CoreLocation",
    "CoreMotion"
  ],
  "dependencies": {
    "Alamofire": [
      "~> 4.9.1"
    ],
    "DataCompression": [
      "~> 3.6.0"
    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "DataCapturing/Tests/**/*.swift"
    },
    {
      "name": "ExampleUnitTests",
      "test_type": "unit",
      "source_files": "ExampleUnitTests/**/*.swift"
    }
  ],
  "swift_version": "5.3"
}
